{
   "name": "prompt_god",
   "private": true,
   "version": "0.0.0",
   "type": "module",
   "scripts": {
      "b": "pnpm --filter @prompt_god/backend",
      "f": "pnpm --filter @prompt_god/webapp",
      "dev": "pnpm --parallel dev",
